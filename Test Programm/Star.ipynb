    "import turtle\n",
    "t = turtle.Turtle()\n",
    "s = turtle.Screen()\n",
    "s.bgcolor('black')\n",
    "t.speed(10)\n",
    "col=['yellow','blue','white','green']\n",
    "c = 0\n",
    "\n",
    "for i in range(50):\n",
    "    t.forward(i*10)\n",
    "    t.right(144)\n",
    "    t.color(col[c])\n",
    "    if c==3:\n",
    "        c=0\n",
    "    else:\n",
    "        c+=1"
   ]
  }
